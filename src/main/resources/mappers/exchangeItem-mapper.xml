<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="ExchangeItem">

	<resultMap id="exchangeItemResultSet" type="com.kh.bookmate.exchange_item.model.vo.ExchangeItem">
		<id column="EXCHANGENO" property="exchangeNo" />
		<result column="PAYMENTDETAILNO" property="paymentDetailNo" />
		<result column="EXCHANGEREASON" property="exchangeReason" />
		<result column="EXCHANGESTATUS" property="exchangeStatus" />
		<result column="EXCHANGENAME" property="exchangeName" />
		<result column="EXCHANGEPHONE" property="exchangePhone" />
		<result column="EXCHANGEADDRESS" property="exchangeAddress" />
	</resultMap>
  
	
	<!-- <select id="selectMyOrderListDetail" resultMap="exchangeItemResultSet">
		SELECT * FROM EXCHANGE_ITEM
		WHERE PAYMENTDETAILNO = ${paymentDetailNo}
	</select> -->
</mapper>
